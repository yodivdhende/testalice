		"@types/bcrypt": "^5.0.2",
		"@types/node": "^22.13.10",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
